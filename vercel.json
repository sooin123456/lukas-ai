{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "framework": "react-router",
  "functions": {
    "app/entry.server.tsx": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/app/entry.server.tsx"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 